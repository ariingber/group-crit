<html lang="en">
  <head>
    <%- include ./partials/head %>
  </head>
<body>
<h3>Group Profile</h3>

<div id="ImageContainer"><%
  name = '';
  groupMembers.forEach(function(member) {
    if (name !== member.name) { %>
    <h3><%= member.name %></h3>
  <% name = member.name } %>
