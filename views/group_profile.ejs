<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include ./partials/head %>
  </head>
<body>

<h2><%= groupMembers[0].groupname %></h2>

<div id="ImageContainer"><%
  name = '';
  groupMembers.forEach(function(member) {
    if (name !== member.name) { %>
    <h3><%= member.name %></h3>
  <% name = member.name } %>
    <h3><%= member.worksname %></h3>
    <a href="/work/<%= member.worksid %>"><img src =<%= member.imgurl %>></a><br />
  <% }) %>
</div>
<div>
  <form method="post" action="/users/logout?_method=DELETE">
    <button>logout</button>
  </form>
</div>


<%- include ./partials/footer %>
</body>
</html>
